/dev/mapper/centos-root /                       xfs     defaults,usrquota,grpquota        0 0
UUID=b23729e3-82c8-4134-ad97-3e63dd398d49 /boot                   xfs     defaults        0 0
/dev/mapper/centos-swap swap                    swap    defaults        0 0
